{
	"realm": "${opencell.keycloak.realm}",
	"auth-server-url": "${opencell.keycloak.url}",
	"ssl-required": "external",
	"resource": "${opencell.keycloak.client}",
	"credentials": {
		"secret": "${opencell.keycloak.secret}"
	},
	"use-resource-role-mappings": true,
	"enable-basic-auth": true,
	"principal-attribute": "preferred_username",
	"autodetect-bearer-only": true
}