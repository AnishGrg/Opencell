<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ws="http://ws.api.meveo.org/">
   <soapenv:Header/>
   <soapenv:Body>
      <ws:createCustomFieldTemplate>
         <!--Optional:-->
         <customField code="${#TestSuite#ticket}" description="${#TestSuite#ticket} NUMBERS">
            <fieldType>STRING</fieldType>
            <accountLevel>SUB</accountLevel>
            <storageType>LIST</storageType>
            <valueRequired>false</valueRequired>
            <versionable>true</versionable>
            <triggerEndPeriodEvent>true</triggerEndPeriodEvent>
            <calendar>MONTHLY</calendar>
         </customField>
      </ws:createCustomFieldTemplate>
   </soapenv:Body>
</soapenv:Envelope>